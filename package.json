{
  "name": "@devrev/ts-adaas",
  "version": "1.8.1-beta.0",
  "description": "Typescript library containing the ADaaS(AirDrop as a Service) control protocol.",
  "type": "commonjs",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "prepare": "npm run build",
    "start": "ts-node src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint .  --fix",
    "test": "jest --forceExit --coverage --runInBand",
    "test:oom": "NODE_OPTIONS=\"--max-old-space-size=256\" node --expose-gc node_modules/.bin/jest --config oom-tests/jest.config.oom.cjs --forceExit --runInBand",
    "test:oom:docker": "./oom-tests/run-oom-tests.sh",
    "test:oom:setup": "./oom-tests/run-oom-tests.sh --skip-tests",
    "test:oom:cleanup": "./oom-tests/run-oom-tests.sh --cleanup-only"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devrev/adaas-sdk.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [],
  "author": "devrev",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^29.5.14",
    "@types/lambda-log": "^3.0.3",
    "@types/node": "^22.15.18",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "axios-mock-adapter": "^2.1.0",
    "aws-sdk": "^2.1691.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.3.3"
  },
  "dependencies": {
    "@devrev/typescript-sdk": "^1.1.59",
    "axios": "^1.11.0",
    "axios-retry": "^4.5.0",
    "form-data": "^4.0.4",
    "js-jsonl": "^1.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "yargs": "^17.7.2"
  },
  "files": [
    "dist"
  ]
}
